{
  "name": "awoooo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --mode=development",
    "build": "cross-env NODE_ENV=production webpack --mode=production",
    "start": "cross-env NODE_ENV=development webpack-dev-server --mode development --open",
    "electron-dev": "concurrently \" yarn start\" \"wait-on http://localhost:3020/ && electron src/electron.js\"",
    "dist": "electron-builder --win -c.extraMetadata.main=dist/electron.js"
  },
  "author": "Dartandr <avv092@gmail.com>",
  "license": "GPL-3.0",
  "browserslist": "> 0.25%, not dead not op_mini all",
  "build": {
    "appId": "com.my-website.my-app",
    "productName": "Awoooo",
    "copyright": "Copyright Â© 2022 ${author}",
    "files": [
      "dist/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets",
      "output": "release/"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/eslint-parser": "^7.17.0",
